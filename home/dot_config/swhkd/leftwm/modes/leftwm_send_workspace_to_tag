super + {1-9}
    @enter leftwm_send_workspace_{0-8}_to_tag && \
    [ -z "$TMPDIR" ] && TMPDIR="/tmp" && \
    printf '%s\n' "leftwm_send_workspace_{0-8}_to_tag " >> \
    "${TMPDIR}/swhkd_$(id --user)"

mode leftwm_send_workspace_0_to_tag
{escape,q}
    @escape{,} && \
    [ -z "$TMPDIR" ] && TMPDIR="/tmp" && \
    sed -i '$d' "${TMPDIR}/swhkd_$(id --user)"

# Move workspace 0 to a tag.
{1-9}
    @escape && \
    [ -z "$TMPDIR" ] && TMPDIR="/tmp" && \
    sed -i '$d' "${TMPDIR}/swhkd_$(id --user)" || exit 0; \

    leftwm-command 'SendWorkspaceToTag 0 {0-8}'
endmode

